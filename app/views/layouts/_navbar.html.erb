<div class=" box nav-wrapper text-color-ltgrey bold">
  <p class="box git-links center">
    <%= link_to "Pull requests  ", "https://git.com/pulls"%>
    <%= link_to "Issues  ", "https://git.com/issues"%>
    <%= link_to "Marketplace  ", "https://git.com/Marketplace"%>
    <%= link_to "explore", "https://git.com/explore"%>
  </p>

    <% if current_user  %>
    <div class="box display-name">
      <p class="text-color-ltgrey"> Logged in as <%= current_user.username %></p>
    </div><!-- <div class="display-name"> -->
      <div class="box login-logout">
      <p><%= link_to "Logout", logout_path %></p>
    <% else %>
      <p class="text-color-blue"><%= link_to "Login with Github!", github_login_path %></p>
    <% end %>
  </div><!-- <div class="login-logout"> -->
</div><!-- <div class="nav-wrapper"> -->
